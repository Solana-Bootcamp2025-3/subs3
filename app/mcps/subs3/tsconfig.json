{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": [
    "src/**/*",
    "../../../target/types/subs3.ts",
    "../../../target/idl/subs3.json"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
